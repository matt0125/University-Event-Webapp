@model StudentNotifications

<h5>RSO's Peniding Approval:</h5>
<br />
@foreach (var item in Model.RsoMembers)
{
    <div class="row">
        <div class="col-md-2">
            @item.Rso.Name
        </div>
        <div class="col-md-4">
            @item.Rso.Description
        </div>
        <div class="col-md-1">
            <a asp-action="ApproveRso" asp-controller="Home" asp-route-id="@item.MemberId" class="btn btn-outline-secondary">Approve</a>
        </div>
        <div class="col-md-1">
            <a asp-action="DenyRso" asp-controller="Home" asp-route-id="@item.MemberId" class="btn btn-outline-secondary">Deny</a>
        </div>

    </div>
}